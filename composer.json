{
    "name": "khatriafaz/laravel-ensure-production",
    "description": "This packages sets the production configuration automatically for laravel projects for the listed domains",
    "type": "library",
    "require": {
        "laravel/framework": "^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Afaz Khatri",
            "email": "khatriafaz@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Khatriafaz\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khatriafaz\\EnsureProductionServiceProvider"
            ]
        }
    }
}
